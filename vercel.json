{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist/sib-fe/browser" }
    },
    {
      "src": "dist/sib-fe/server/server.mjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/sib-fe/server/server.mjs"
    }
  ]
}
